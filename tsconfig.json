{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/VerticalCarouselSecond/VerticalCarouselSecond.js", "components/LookAtCursor/LookAtCursor.js", "assets/Icons/magnifier.jsx", "components/3DSlider/ImageSlider.jsx", "components/ProductSliderContainer/ProductSliderContainer.jsx"],
  "exclude": ["node_modules"]
}
